	"monitoring": {
		"settings": {
			"intervalMillis": "30000"
		},
		"tests": {
			"tcp": {
				"timeoutMillis": "5000"
			},
			"web": {
				"timeoutMillis": "10000"
			}
		},
		"notify": {
			"user1": {
				"type": "email",
				"name": "User 1",
				"email": "user1@example.com"
			},
			"user2": {
				"type": "email",
				"name": "User 2",
				"email": "user2@example.com"
			},
			"user3": {
				"type": "email",
				"name": "User 3",
				"email": "user3@example.com"
			},
			"grp-adm": {
				"type": "group",
				"name": "Admins",
				"list": [
					"user1",
					"user2",
					"user3"
				]
			}
		}
	},
