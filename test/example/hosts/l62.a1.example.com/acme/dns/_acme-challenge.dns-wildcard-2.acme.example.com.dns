; Zone: _acme-challenge.dns-wildcard-2.acme.example.com.

$ORIGIN _acme-challenge.dns-wildcard-2.acme.example.com.
$TTL 15

; SOA Record
@ IN SOA l62.a1.example.com. user.example.com. (
    15      ;refresh
    10      ;retry
    60480   ;expire
    15      ;minimum ttl
)

@ IN NS l62.a1.example.com.

@            IN      TXT     "empty"
vendor       IN      TXT     beaver-l6route
