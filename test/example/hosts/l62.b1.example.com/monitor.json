            "email": "user3@example.com"
        },
        "slack-alert": {
            "type": "slack",
            "filter": [
                "error",
                "alert"
            ],
            "auth": "TOKEN",
            "hook": "https://slack.com/api/chat.postMessage?channel=bot-messages"
        },
        "slack-track": {
            "type": "slack",
            "hook": "HOOK_URL",
            "disabled": true
        },
        "grp-adm": {
            "type": "group",
