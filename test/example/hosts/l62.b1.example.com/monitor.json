{
    "disabled": false,
    "testing": false,
    "appendReason": false,
    "appendReasonTcp": true,
    "appendReasonWeb": true,
    "interval": 30000,
    "tcpTimeout": 1000,
    "webTimeout": 1000,
    "maxAttempts": 1,
    "notify": {
        "user1": {
            "type": "email",
            "name": "User 1",
            "email": "user1@example.com"
        },
        "user2": {
            "type": "email",
